{
  "name": "melt",
  "productName": "Melt",
  "version": "0.1.0",
  "description": "A markdown editor in which the brain melts.",
  "main": "main.js",
  "dependencies": {
    "axios": "0.18.0",
    "codemirror": "5.35.0",
    "element-ui": "2.2.0",
    "github-markdown-css": "2.10.0",
    "highlight.js": "9.12.0",
    "marked": "0.3.16",
    "node-emoji": "1.8.1",
    "vue": "2.5.13",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "css-loader": "0.28.10",
    "electron": "1.8.2",
    "electron-packager": "11.0.1",
    "eslint": "4.18.1",
    "file-loader": "1.1.10",
    "node-sass": "4.7.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.2",
    "vue-loader": "14.1.1",
    "vue-template-compiler": "2.5.13",
    "webpack": "4.0.1",
    "webpack-cli": "2.0.9"
  },
  "scripts": {
    "start": "electron .",
    "dev": "NODE_ENV=development webpack --watch",
    "prepack:mac": "NODE_ENV=production webpack",
    "pack:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./icon.icns --prune=true --out=release-builds"
  },
  "keywords": [
    "markdown",
    "editor"
  ],
  "author": "qazsato",
  "license": "MIT"
}
